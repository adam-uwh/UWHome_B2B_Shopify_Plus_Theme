    "rich_text_GPraHE": {
      "type": "rich-text",
      "blocks": {
        "heading_qW67q9": {
          "type": "heading",
          "settings": {
            "heading": "Direct to Consumer",
            "heading_size": "h1"
          }
        },
        "text_3tXYty": {
          "type": "text",
          "settings": {
            "text": "<p>Our DSV (Drop Ship Vendor) service lets trade partners sell direct-to-consumer without the inventory or shipping headaches. You share customer orders with us and we fulfil, pack and dispatch them straight to the end customer under your instructions.</p>"
          }
        },
        "heading_hRNQqf": {
          "type": "heading",
          "settings": {
            "heading": "Why Use Our DSV Service?",
            "heading_size": "h2"
          }
        },
        "text_LXERcP": {
          "type": "text",
          "settings": {
            "text": "<ul><li><strong>Lower overheads:</strong> no need to hold or stage additional products for delivery.</li><li><strong>Faster delivery:</strong> orders move directly from our warehouses to the customer, reducing transit and handling time.</li><li><strong>Scalability:</strong> handle seasonal spikes, promotional campaigns and national rollouts without hiring seasonal staff or extra warehousing.</li><li><strong>Brand control:</strong> we can ship using courier labels with your name and logo and include your customised customer dispatch notes.</li><li><strong>Reduced returns complexity:</strong> consolidated returns handling and clear reconciliation for trade customers.</li><li><strong>Expert handling:</strong> handled by teams experienced in careful textiles packing, protective covers and quality checks on every order.</li></ul>"
          }
        },
        "button_88mceQ": {
          "type": "button",
          "disabled": true,
          "settings": {
            "button_label": "Request an Account",
            "button_link": "shopify://pages/company-account-request",
            "button_style_secondary": false,
            "button_label_2": "",
            "button_link_2": "",
            "button_style_secondary_2": false
          }
        }
      },
